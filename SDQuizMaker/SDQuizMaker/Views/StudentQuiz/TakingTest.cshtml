@model IEnumerable<SDQuizMaker.Models.tbtquestion>
    @{
    ViewBag.Title = "TakingTest";
    Layout = "~/Views/Shared/_Layout.cshtml";
    int i = 1;
    var tempname = (string)Session["tempname"];
    }
    <h2>Active Test - [@tempname]</h2>

    <table class="table">
        <tr>
<th></th>
            <th>
                @Html.DisplayNameFor(model => model.Question)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Explanation)
            </th>
            <th></th>
        </tr>
        @foreach (var item in Model) {
        <tr>
<td>@i.</td>
            <td>
                @Html.DisplayFor(modelItem => item.Question)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Explanation)
            </td>
            <td>
                @Html.ActionLink("Answer","TestAnswers","StudentQuiz", new { id=item.TQuestionID }, null)
            </td>
        </tr>
i++;
        }
    </table>
